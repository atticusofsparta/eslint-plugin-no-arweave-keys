{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "no-arweave-keys"
  ],
  "rules": {
    "no-arweave-keys/no-arweave-keys": "error"
  }
}
